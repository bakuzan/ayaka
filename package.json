{
  "name": "bkz-ayaka",
  "version": "3.0.0",
  "description": "Bakuzan's Function Library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bakuzan/ayaka.git"
  },
  "main": "index.js",
  "files": [
    "*",
    "!*.tgz"
  ],
  "lint-staged": {
    "{,lib,tools,__tests__}/**/*.ts": [
      "npm run lint"
    ],
    "{,lib,tools,__tests__}/**/*.{js,html,scss}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint -c .eslintrc.js --ext .ts ./lib --fix",
    "lint:staged": "lint-staged",
    "test": "npx jest",
    "test:watch": "npx jest -o --watch",
    "test:coverage": "npx jest --coverage",
    "build": "tsc --build && node ./tools/postBuild.js",
    "docs": "typedoc --out docs lib",
    "disable-jekyll": "touch ./docs/.nojekyll",
    "deploy": "rmdir /s /q docs && npm run docs && npm run disable-jekyll && gh-pages --dotfiles -d docs",
    "release:test": "npm run build && cd .\\build && npm pack && cd ..",
    "release:npm": "npm run build && cd .\\build && npm publish && cd .."
  },
  "keywords": [
    "typescript",
    "function library"
  ],
  "author": "bakuzan",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@types/jest": "^29.0.1",
    "@types/node": "^18.7.16",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/eslint-plugin-tslint": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "babel-core": "^7.0.0-bridge.0",
    "chalk": "^4.1.2",
    "dotenv": "^16.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^34.8.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "execa": "^4.1.0",
    "gh-pages": "^3.2.3",
    "husky": "^8.0.1",
    "import": "0.0.6",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "jest-localstorage-mock": "^2.4.22",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.0",
    "tslib": "^2.4.0",
    "typedoc": "^0.23.14",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.0"
  }
}
