{
  "name": "meiko",
  "version": "0.5.0",
  "private": true,
  "description": "Component Library",
  "main": "dist/bundle.min.js",
  "module": "dist/bundle.esm.js",
  "browser": "dist/bundle.umd.js",
  "style": "dist/bundle.css",
  "files": ["dist"],
  "lint-staged": {
    "{,!(dist|.git|node_modules)/**/}*.{js,jsx,json,scss}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "pre-commit": ["precommit-msg", "lint"],
  "scripts": {
    "lint": "lint-staged",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prep-templates": "node copy-templates.js",
    "build": "rollup -c",
    "build-and-prep": "npm run build && npm run prep-templates",
    "storybook": "start-storybook -p 7200 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "keywords": ["reactjs", "component-library"],
  "author": "bakuzan",
  "license": "ISC",
  "devDependencies": {
    "@dump247/storybook-state": "^1.5.0",
    "@storybook/react": "^3.4.1",
    "@storybook/storybook-deployer": "^2.3.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.11",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.46.2",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.7.0",
    "fetch-mock": "^6.3.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "node-sass-chokidar": "^1.2.2",
    "postcss-loader": "^2.1.3",
    "pre-commit": "^1.2.2",
    "prettier": "^1.11.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-includepaths": "^0.2.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.1",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  }
}
