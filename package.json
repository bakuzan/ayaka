{
  "name": "bkz-ayaka",
  "version": "3.0.0",
  "description": "Bakuzan's Function Library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bakuzan/ayaka.git"
  },
  "main": "index.js",
  "files": [
    "*",
    "!*.tgz"
  ],
  "lint-staged": {
    "{,lib,tools,__tests__}/**/*.{ts,tsx,js,jsx,json}": [
      "prettier --single-quote --write"
    ]
  },
  "pre-commit": [
    "precommit-msg",
    "lint-ts",
    "pretty",
    "test"
  ],
  "scripts": {
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "lint-ts": "tslint -c tslint.json 'lib/**/*.{ts,tsx}'",
    "pretty": "lint-staged",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "test": "npx jest",
    "test:watch": "npx jest -o --watch",
    "test:coverage": "npx jest --coverage",
    "build:bundle": "rollup -c",
    "build:local": "tsc --build && node ./tools/postBuild.js",
    "docs": "typedoc --out docs lib",
    "disable-jekyll": "touch ./docs/.nojekyll",
    "deploy": "rmdir /s /q docs && npm run docs && npm run disable-jekyll && gh-pages --dotfiles -d docs",
    "release:test": "npm run build:local && cd .\\build && npm pack && cd ..",
    "release:npm": "npm run build:local && cd .\\build && npm publish && cd .."
  },
  "keywords": [
    "typescript",
    "function library"
  ],
  "author": "bakuzan",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.6",
    "@rollup/plugin-replace": "^2.2.1",
    "@types/jest": "^25.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "chalk": "^2.4.2",
    "dotenv": "^8.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^21.0.0",
    "execa": "^1.0.0",
    "gh-pages": "^2.0.1",
    "husky": "^4.2.3",
    "import": "0.0.6",
    "jest": "^25.1.0",
    "jest-localstorage-mock": "^2.4.0",
    "lint-staged": "^10.0.8",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1",
    "rollup": "^1.27.9",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^9.2.2",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^25.2.1",
    "tslib": "^1.11.1",
    "typedoc": "^0.16.11",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4"
  }
}
