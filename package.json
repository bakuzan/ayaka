{
  "name": "ayaka",
  "version": "0.1.0",
  "private": true,
  "description": "Function Library",
  "main": "index.js",
  "browser": "dist/ayaka.min.js",
  "module": "dist/module.min.js",
  "files": [
    "build",
    "dist"
  ],
  "lint-staged": {
    "{,lib,tools,__tests__}/**/*.{ts,tsx,js,jsx,json}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "pre-commit": [
    "precommit-msg",
    "lint-ts",
    "pretty",
    "test"
  ],
  "scripts": {
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "lint-ts": "tslint -c tslint.json 'lib/**/*.{ts,tsx}'",
    "pretty": "lint-staged",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "test": "npx jest",
    "test-w": "npx jest --watch",
    "coverage": "npx jest --coverage",
    "bundle": "rollup -c",
    "build": "tsc --build && node ./tools/postBuild.js",
    "build-all": "npm run bundle && npm run build"
  },
  "keywords": [
    "function library"
  ],
  "author": "bakuzan",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.0.0",
    "@types/jest": "^24.0.9",
    "babel-core": "^7.0.0-bridge.0",
    "chalk": "^2.4.2",
    "dotenv": "^6.2.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-localstorage-mock": "^2.4.0",
    "lint-staged": "^8.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.4",
    "rollup": "^1.4.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-plugin-uglify": "^6.0.2",
    "ts-jest": "^24.0.0",
    "tslib": "^1.9.3",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4"
  }
}
